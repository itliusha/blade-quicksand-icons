{
    "name": "itliusha/blade-quicksand-icons",
    "description": "A package to easily make use of Heroicons in your Laravel Blade views.",
    "keywords": ["Blade", "quicksand", "Laravel"],
    "homepage": "https://github.com/itliusha/blade-quicksand-icons",
    "license": "MIT",
    "authors": [
        {
            "name": "itliusha",
            "email": "itliusha@qq.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "blade-ui-kit/blade-icons": "^1.7",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.5|^11.0"
    },
    "autoload": {
        "psr-4": {
            "BladeUI\\QuicksandIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BladeUI\\QuicksandIcons\\BladeQuicksandIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
