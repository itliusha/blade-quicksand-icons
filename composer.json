{
    "name": "itliusha/blade-quicksand",
    "description": "A package to easily make use of Heroicons in your Laravel Blade views.",
    "keywords": ["Blade", "quicksand", "Laravel"],
    "homepage": "https://github.com/itliusha/blade-quicksand",
    "license": "MIT",
    "authors": [
        {
            "name": "itliusha",
            "email": "itliusha@qq.com"
        }
    ],
    "require": {
        "php": "~8.2.0|~8.3.0",
        "blade-ui-kit/blade-icons": "^1.6",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "BladeUI\\Quicksand\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BladeUI\\Quicksand\\BladeQuicksandServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
